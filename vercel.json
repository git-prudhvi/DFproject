{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/encode", "destination": "/index.html" },
    { "source": "/decode", "destination": "/index.html" }
  ]
}


